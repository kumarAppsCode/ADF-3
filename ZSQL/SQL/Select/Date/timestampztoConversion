select 
EFFECTIVE_START_DATE,
TO_DATE(SUBSTR(EFFECTIVE_START_DATE, 1, 19), 'YYYY-MM-DD"T"HH24:MI:SS') as ddw,
TO_DATE (EFFECTIVE_START_DATE, 'yyyy-mm-dd hh24:mi:ss') as dd,
to_char(to_timestamp_tz(EFFECTIVE_START_DATE, 'YYYY-MM-DD"T"HH24:MI:SS.ff3TZH:TZM'),'YYYY-MM-DD HH24:MI:SS') as abc,
to_Date(to_char(to_timestamp_tz(EFFECTIVE_START_DATE, 'YYYY-MM-DD"T"HH24:MI:SS.ff3TZH:TZM'),'YYYY-MM-DD HH24:MI:SS'),'YYYY-MM-DD HH24:MI:SS') as charq
from PER_ALL_ASSIGNMENTS_F
