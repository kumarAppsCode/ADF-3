SELECT
 lt.LOOKUP_TYPE_ID
,lt.LOOKUP_TYPE_NAME
,lt.LOOKUP_TYPE_NAME_DISP
,lt.LOOKUP_TYPE_NAME_DISP_TL
,lt.ACCESS_LEVEL
,lt.ACTIVE_YN
,lt.CREATED_BY
,lt.CREATION_DATE
,lt.LAST_UPDATED_BY
,lt.LAST_UPDATE_DATE
,lt.LAST_UPDATE_LOGIN
,(select LISTAGG(LOOKUP_VALUE_NAME, ',') WITHIN GROUP (ORDER BY LOOKUP_VALUE_NAME) from XX_FND_LOOKUP_VALUES   
where LOOKUP_TYPE_ID=lt.LOOKUP_TYPE_ID)  AS LOOKUP_VALUE_NAME
--,LISTAGG(lv.LOOKUP_VALUE_NAME, ',') WITHIN GROUP (ORDER BY lv.LOOKUP_VALUE_NAME) AS LOOKUP_VALUE_NAME
--,LISTAGG(lv.LOOKUP_VALUE_NAME_DISP, ',') WITHIN GROUP (ORDER BY lv.LOOKUP_VALUE_NAME_DISP) AS LOOKUP_VALUE_NAME_DISP
FROM XX_FND_LOOKUP_TYPES lt, XX_FND_LOOKUP_VALUES lv
where lt.LOOKUP_TYPE_ID=lv.LOOKUP_TYPE_ID
--and lt.LOOKUP_TYPE_ID=100137
GROUP BY
lt.LOOKUP_TYPE_ID,
lt.LOOKUP_TYPE_NAME,
lt.LOOKUP_TYPE_NAME_DISP,
lt.LOOKUP_TYPE_NAME_DISP_TL,
lt.ACCESS_LEVEL,
lt.ACTIVE_YN,
lt.CREATED_BY,
lt.CREATION_DATE,
lt.LAST_UPDATED_BY,
lt.LAST_UPDATE_DATE,
lt.LAST_UPDATE_LOGIN;
