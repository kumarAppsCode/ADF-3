select * FROM IGG_XX_OTL_BATCH_HEADER 
WHERE 
BATCH_HDR_ID=BATCH_HDR_ID+0
and BATCH_HDR_ID in (
115326,
115347,
115348)
FOR UPDATE NOWAIT;


